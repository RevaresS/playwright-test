{
  "name": "orders",
  "version": "1.0.0",
  "description": "UI-тесты",
  "scripts": {
    "tests": "npm run allure:clean; npm run tests:desktop; npm run tests:mobile",
    "allure:clean": "rm -rf ./allure-results ./allure-report ./test-results ./trace ./junit.xml",
    "allure:generate": "allure generate ./allure-results --clean",
    "allure:open": "allure open ./allure-report",
    "allure:serve": "allure serve ./allure-results",
    "eslint": "npx eslint --resolve-plugins-relative-to ./node_modules/@fe/eslint-config --ext .js,.ts,.tsx,.graphql .",
    "lint-staged": "npx lint-staged",
    "eslint:fix": "npm run eslint -- --fix",
    "pw:show-trace": "npx playwright show-trace --port 0",
    "pw:ui-mode": "npx playwright test --ui-port 0",
    "api": "npx @fe/api"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.ozon.ru:internal-projects/profit-edu-ui-tests.git"
  },
  "devDependencies": {
    "@fe/api": "^3.11.1",
    "@fe/eslint-config": "^6.1.0",
    "@fe/lint-staged-config": "^1.1.0",
    "@fe/tsconfig": "^1.1.0",
    "@playwright/test": "^1.51.1",
    "@types/qs": "^6.9.15",
    "@types/validator": "^13.7.12",
    "allure-playwright": "^2.11.1",
    "csv-parse": "^5.3.1",
    "eslint": "^8.56.0",
    "lint-staged": "^13.2.3",
    "playwright": "^1.51.1",
    "typescript": "^4.9.5",
    "validator": "^13.11.0"
  },
  "dependencies": {
    "@bx-fe/vault-client": "^1.1.1",
    "@fe/partials-nuxt3": "^1.2.4",
    "axios": "^1.6.8",
    "date-fns": "^3.6.0",
    "exceljs": "^4.4.0",
    "qs": "^6.12.1",
    "xlsx": "^0.20.1"
  }
}
